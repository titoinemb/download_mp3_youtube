{
  "name": "download_mp3_youtube",
  "version": "1.1.1",
  "main": "index.js",
  "description": "This application allows you to install playlists as well as a list of videos/playlists of music from YouTube",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager . download_mp3_youtube --platform=win32 --arch=x64 --out=dist --overwrite --icon=icon.ico"
  },
  "keywords": [],
  "author": "titoinemb",
  "license": "ISC",
  "devDependencies": {
    "electron": "^33.2.0",
    "electron-packager": "^17.1.2",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-updater": "^6.3.9"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "child_process": "^1.0.2",
    "custom-electron-titlebar": "^4.2.8",
    "express": "^4.21.2",
    "iconv-lite": "^0.6.3",
    "node-fetch": "^3.3.2",
    "node-id3": "^0.2.6",
    "puppeteer": "^23.11.1"
  },
  "build": {
    "appId": "com.app.downloadmp3",
    "productName": "Download MP3 YouTube",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "node_modules/",
      "src/"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "titoinemb",
        "repo": "download_mp3_youtube"
      }
    ]
  }
}
